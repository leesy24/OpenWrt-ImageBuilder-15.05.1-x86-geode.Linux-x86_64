
config wifi-device 'radio0'
        option type 'mac80211'
        option hwmode '11g'
        option path 'pci0000:00/0000:00:0c.0'
        option country 'KR'
        option htmode 'HT40'
        option channel 'auto'

config wifi-iface
        option device 'radio0'
        option network 'lan'
        option mode 'ap'
        option ssid 'AP01'
        option encryption 'none'
	option wds '1'

config wifi-device 'radio1'
        option type 'mac80211'
        option hwmode '11g'
        option path 'pci0000:00/0000:00:0e.0'
        option country 'KR'
        option htmode 'HT40'
        option channel 'auto'

config wifi-iface
        option device 'radio1'
        option network 'lan'
        option mode 'sta'
        option ssid 'AP02'
        option encryption 'none'
        option key '1'
        option key1 's:string13chars'
	option wds '1'


